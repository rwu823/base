  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "babel",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "args": [
        "${relativeFile}"
      ]
    },

    {
      "type": "node",
      "request": "launch",
      "name": "ts",
      "args": [
        "--require",
        "ts-node/register",
        "${relativeFile}",
      ]
    }
  ]
}
